<oaf_info>

<oaf_server iid="OAFIID:gchem3d_factory" type="exe" location="gchem3d">
		<oaf_attribute name="repo_ids" type="stringv">
	<item value="IDL:GNOME/GenericFactory:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Gnome Chemistry 3D Viewer embeddable factory"/>
	<oaf_attribute name="description" type="string" _value="Gnome Chemistry 3D Viewer object factory"/>
</oaf_server>

<oaf_server iid="OAFIID:gchem3d_control" type="factory" location="OAFIID:gchem3d_factory">
	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:Bonobo/Unknown:1.0"/>
		<item value="IDL:Bonobo/Control:1.0"/>
		<item value="IDL:Bonobo/PersistStream:1.0"/>
		<item value="IDL:Bonobo/Persist:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Gnome Chemistry 3D Viewer component"/>
	<oaf_attribute name="description" type="string" _value="Gnome Chemistry 3D Viewer component to display molecular structures"/>
	<oaf_attribute name="bonobo:editable" type="boolean" value="false"/>
	<oaf_attribute name="bonobo:supported_mime_types" type="stringv">
		<item value="chemical/x-xyz"/>
		<item value="chemical/x-pdb"/>
		<item value="chemical/x-mdl-molfile"/>
	</oaf_attribute>
	<oaf_attribute name="nautilus:view_as_name" type="string" _value="Molecular Model"/>
</oaf_server>

</oaf_info>
