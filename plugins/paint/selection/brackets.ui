<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkTable" id="brackets">
    <property name="visible">True</property>
    <property name="n_rows">2</property>
    <property name="n_columns">2</property>
    <property name="column_spacing">10</property>
    <property name="row_spacing">6</property>
    <child>
      <object class="GtkLabel" id="type-lbl">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Brackets type:</property>
      </object>
      <packing>
        <property name="x_options"></property>
        <property name="y_options"></property>
      </packing>
    </child>
    <child>
      <object class="GtkLabel" id="used-lbl">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Used brakets:</property>
      </object>
      <packing>
        <property name="top_attach">1</property>
        <property name="bottom_attach">2</property>
        <property name="x_options"></property>
        <property name="y_options"></property>
      </packing>
    </child>
    <child>
      <object class="GtkComboBox" id="type-box">
        <property name="visible">True</property>
        <property name="model">type-list</property>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="right_attach">2</property>
        <property name="x_options">GTK_EXPAND</property>
        <property name="y_options"></property>
      </packing>
    </child>
    <child>
      <object class="GtkComboBox" id="used-box">
        <property name="visible">True</property>
        <property name="model">used-list</property>
      </object>
      <packing>
        <property name="left_attach">1</property>
        <property name="right_attach">2</property>
        <property name="top_attach">1</property>
        <property name="bottom_attach">2</property>
        <property name="x_options">GTK_EXPAND</property>
        <property name="y_options"></property>
      </packing>
    </child>
  </object>
  <object class="GtkListStore" id="type-list">
    <columns>
      <!-- column-name text -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">()</col>
      </row>
      <row>
        <col id="0" translatable="yes">[]</col>
      </row>
      <row>
        <col id="0" translatable="yes">{}</col>
      </row>
    </data>
  </object>
  <object class="GtkListStore" id="used-list">
    <columns>
      <!-- column-name text -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Both</col>
      </row>
      <row>
        <col id="0" translatable="yes">Opening</col>
      </row>
      <row>
        <col id="0" translatable="yes">Closing</col>
      </row>
    </data>
  </object>
</interface>
